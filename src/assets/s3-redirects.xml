<RoutingRules>
  <RoutingRule>
    <Condition>
      <KeyPrefixEquals>cn/</KeyPrefixEquals>
      <HttpErrorCodeReturnedEquals>404</HttpErrorCodeReturnedEquals>
    </Condition>
    <Redirect>
      <Protocol>https</Protocol>
      <HostName>${ENV_DOMAIN}invest.great.gov.uk</HostName>
      <ReplaceKeyWith>cn/404.html</ReplaceKeyWith>
    </Redirect>
  </RoutingRule>
